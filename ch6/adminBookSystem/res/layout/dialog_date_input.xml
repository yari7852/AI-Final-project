<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
	<LinearLayout android:id="@+id/LinearLayout01" 
	    android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<ImageButton android:id="@+id/ImageButton01" 
		    android:background="@drawable/up"
		    android:layout_marginLeft="0dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/ImageButton02"
		    android:background="@drawable/down"
		    android:layout_marginLeft="10dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/ImageButton03" 
		    android:background="@drawable/up"
		    android:layout_marginLeft="10dip"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/ImageButton04" 
		    android:background="@drawable/down"
		    android:layout_marginLeft="10dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/ImageButton05" 
		    android:background="@drawable/up"
		    android:layout_marginLeft="10dip"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content">
		</ImageButton>
		<ImageButton android:id="@+id/ImageButton06" 
		    android:background="@drawable/down"
		    android:layout_marginLeft="10dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</ImageButton>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout02" 
	    android:orientation="horizontal"
	    android:layout_marginTop="5dip"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<EditText android:text="" 
			android:id="@+id/EditText01" 
		    android:layout_marginLeft="10dip"
			android:layout_width="70dip" 
			android:layout_height="40dip">
		</EditText>
		<TextView android:text="年" 
		    android:textSize="16dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</TextView>
		<EditText android:text="" 
			android:id="@+id/EditText02" 
		    android:layout_marginLeft="10dip"
			android:layout_width="70dip" 
			android:layout_height="40dip">
		</EditText>
		<TextView android:text="月" 
		    android:textSize="16dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</TextView>
		<EditText android:text="" 
			android:id="@+id/EditText03" 
		    android:layout_marginLeft="10dip"
			android:layout_width="70dip" 
			android:layout_height="40dip">
		</EditText>
		<TextView android:text="日" 
		    android:textSize="16dip"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
		</TextView>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout03" 
	    android:layout_marginTop="5dip"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<Button android:text="確定" 
			android:id="@+id/Button01" 
		    android:textSize="16dip"
		    android:layout_marginLeft="60dip"
			android:layout_width="80dip" 
			android:layout_height="wrap_content">
		</Button>
		<Button android:text="返回" 
			android:id="@+id/Button02" 
		    android:textSize="16dip"
		    android:layout_marginLeft="30dip"
			android:layout_width="80dip" 
			android:layout_height="wrap_content">
		</Button>
	</LinearLayout>
</LinearLayout>
